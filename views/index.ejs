        <script>
            function animateHeroContent() {
                const heroContent = document.querySelector('.hero-content');
                const subtitle = document.querySelector('.hero-subtitle');
                const title = document.querySelector('.hero-title');
                const text = document.querySelector('.hero-text');
                
                subtitle.style.left = "0";
                subtitle.style.opacity = "1";
                
                setTimeout(() => {
                    title.style.left = "0";
                    title.style.opacity = "1";
                }, 500); // 0.5 seconds delay for title
        
                setTimeout(() => {
                    text.style.left = "0";
                    text.style.opacity = "1";
                }, 1000); // 1 second delay for text
        
                setTimeout(() => {
                    heroContent.style.transition = "all 2s ease-in-out";
                    heroContent.style.left = "-100%";
                }, 10000); // Stay for 10 seconds
        
                setTimeout(() => {
                    heroContent.style.left = "0";
                }, 12000); // Move back after 2 seconds
            }
        
            window.onload = animateHeroContent;
        </script>
        

